        "/account/zypt_info/": {
            "get": {
                "description": "获取自营平台的信息",
                "parameters": [

                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "code": {
