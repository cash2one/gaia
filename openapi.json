        "/mall/supplier/": {
          "put": {
            "description": "添加供货商",
            "parameters": [
              {
                  "name": "owner_id",
                  "in": "query",
                  "description": "对应自营平台user id",
                  "required": true,
                  "type": "integer"
              },
              {
                  "name": "name",
                  "in": "query",
                  "description": "供货商的名称",
                  "required": true,
                  "type": "string"
              },
              {
                  "name": "responsible_person",
                  "in": "query",
                  "description": “负责人",
                  "required": true,
                  "type": "string"
              },
              {
                "name": "supplier_tel",
                "in": "query",
                "description": “供货商电话",
                "required": true,
                "type": "string"
              },
              {
                "name": "supplier_address",
                "in": "query",
                "description": “供货商地址",
                "required": true,
                "type": "string"
              },
              {
                "name": "remark",
                "in": "query",
                "description": “备注",
                "required": false,
                "type": "string"
              }
            ],
            "responses": {
              "200": {
                "description": "Successful response",
                "schema": {
                    "type": "object",
                    "properties": {
                        "code": {
                            "type": "integer",
                            "format": "int32"
                        },
                        "data": {
                            "type": "object",
                            "properties": {
                                "msg": {
                                    "type": "string",
                                    "description": "返回信息"
                                },
                                "supplier_id": {
                                    "type": "integer",
                                    "description": "创建的供货商的id"
                                }
                            }
                        },
                        "errMsg": {
                            "type": "string"
                        },
                        "innerErrMsg": {
                            "type": "string"
                        }
                    }
                }
              }
            }
          }
        },
        "/mall/product/": {
          "put": {
            "description": "创建商品",
            "parameters":[
              {
                "name": "owner_id",
                "in": "query",
                "description": "商品所属账号的user id",
                "required": true,
                "type": "integer"
              },
              {
                "name": "name",
                "in": "query",
                "description": "商品名称",
                "required": true,
                "type": "string"
              },
              {
                "name": "supplier",
                "in": "query",
                "description": "商品供货商id",
                "required": false,
                "type": "integer"
              },
              {
                "name": "purchase_price",
                "in": "query",
                "description": "商品采购价",
                "required": false,
                "type": "string"
              },
              {
                "name": "promotion_title",
                "in": "query",
                "description": "商品促销标题",
                "required": false,
                "type": "string"
              },
              {
                "name": "user_code",
                "in": "query",
                "description": "商品编码",
                "required": false,
                "type": "string"
              },
              {
                "name": "bar_code",
                "in": "query",
                "description": "商品条码",
                "required": false,
                "type": "string"
              },
              {
                "name": "product_category",
                "in": "query",
                "description": "将商品放入的分组当中",
                "required": false,
                "type": "string"
              },
              {
                "name": "min_limit",
                "in": "query",
                "description": "限购数量",
                "required": false,
                "type": "integer"
              },
              {
                "name": "is_member_product",
                "in": "query",
                "description": "是否参与会员折扣",
                "required": false,
                "type": "boolean"
              },
              {
                "name": "customModels",
                "in": "query",
                "description": "定制规格json字符窜",
                "required": false,
                "type": "string"
              },
              {
                "name": "is_use_custom_model",
                "in": "query",
                "description": "商品是否使用订制规格",
                "required": true,
                "type": "boolean"
              },
              {
                "name": "price",
                "in": "query",
                "description": "商品单价",
                "required": false,
                "type": "string"
              },
              {
                "name": "weight",
                "in": "query",
                "description": "商品重量",
                "required": false,
                "type": "string"
              },
              {
                "name": "stock_type",
                "in": "query",
                "description": "库存类型（有限或者无限）",
                "required": true,
                "type": "boolean"
              },
              {
                "name": "stocks",
                "in": "query",
                "description": "库存值",
                "required": false,
                "type": "integer"
              },
              {
                "name": "swipe_images",
                "in": "query",
                "description": "商品图片（json字符串）",
                "required": true,
                "type": "string"
              },
              {
                "name": "postage_type",
                "in": "query",
                "description": "运费类型",
                "required": true,
                "type": "string"
              },
              {
                "name": "unified_postage_money",
                "in": "query",
                "description": "统一运费金额",
                "required": true,
                "type": "string"
              },
              {
                "name": "is_enable_cod_pay_interface",
                "in": "query",
                "description": "是否支持货到付款",
                "required": false,
                "type": "boolean"
              },
              {
                "name": "is_use_online_pay_interface",
                "in": "query",
                "description": "是否支持在线支付",
                "required": false,
                "type": "boolean"
              },
              {
                "name": "is_enable_bill",
                "in": "query",
                "description": "是否支持开发票",
                "required": false,
                "type": "boolean"
              },
              {
                "name": "is_delivery",
                "in": "query",
                "description": "是否支持配送时间",
                "required": false,
                "type": "boolean"
              },
              {
                "name": "properties",
                "in": "query",
                "description": "商品属性（json）",
                "required": false,
                "type": "string"
              },
              {
                "name": "detail",
                "in": "query",
                "description": "商品详情",
                "required": false,
                "type": "string"
              }
            ],
            "responses": {
              "200": {
                "description": "Successful response"
              }
            }
          }
        },
        "/mall/user_list": {
          "get": {
                "description": "商户列表",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "需要第几页的数据，默认第一页",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "name": "page_count",
                        "in": "query",
                        "description": "每一页需要多少个商户数据，默认15条",
                        "required": true,
                        "type": "integer"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "code": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "data": {
                                    "type": "object",
                                    "properties": {
                                        "users": {
                                            "type": "array",
                                            "description": "商户集合",
                                            "items":{
                                                "type": "object",
                                                "properties": {
                                                   "user_id": {
                                                      "type": "integer",
                                                      "description": "商户的user_id"

                                                    },
                                                    "webapp_id": {
                                                      "type": "string",
                                                      "description": "商户的weapp_id"
                                                    },
                                                    "store_name": {
                                                      "type": "string",
                                                      "description": "商户的商店名"

                                                    }
                                                }
                                            }
                                        },
                                        "counts":{
                                          "type": "integer",
                                          "description": "总共有多少个有效的商户"
                                        }

                                    }
                                },
                                "errMsg": {
                                    "type": "string"
                                },
                                "innerErrMsg": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
         "/mall/order_list/": {
            "get": {
                "description": "获取订单列表",
                "parameters": [
                    {
                        "name": "owner_id",
                        "in": "query",
                        "description": "商品所属账号的user id",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "cur_page",
                        "in": "query",
                        "description": "分页器当前请求页",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "name": "count_per_page",
                        "in": "query",
                        "description": "分页器每页显示数量",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "name": "ship_name",
                        "in": "query",
                        "description": "发货地址",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "ship_tel",
                        "in": "query",
                        "description": "收货电话",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "product_name",
                        "in": "query",
                        "description": "商品名称",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "pay_type",
                        "in": "query",
                        "description": "支付方式：参考weapp.mall.models.PAYTYPE2NAME",
                        "required": false,
                        "type": "integer"
                    },
                    {
                        "name": "express_number",
                        "in": "query",
                        "description": "快递号",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "source",
                        "in": "query",
                        "description": "订单来源：1:商城；0本店",
                        "required": false,
                        "type": "integer"
                    },
                    {
                        "name": "status",
                        "in": "query",
                        "description": "订单状态：  0  待支付：已下单，未付款  \n1  已取消：取消订单(回退销量)  \n 2  已支付：已下单，已付款，已不存此状态  \n3  待发货：已付款，未发货  \n4  已发货：已付款，已发货  \n5  已完成：为已完成状态  \n6  退款中  \n7  退款完成(回退销量)   \n8  团购退款（没有退款完成按钮）  \n9  团购退款完成",
                        "required": false,
                        "type": "integer"
                    },
                    {
                        "name": "is_used_weizoom_card",
                        "in": "query",
                        "description": "是否使用微众卡",
                        "required": false,
                        "type": "boolean"
                    },
                    {
                        "name": "date_interval",
                        "in": "query",
                        "description": "查询时间：2014-10-07|2014-10-08，注：使用‘|’区分，该参数和date_interval_type结合使用",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "date_interval_type",
                        "in": "query",
                        "description": "时间类型：1 下单时间 2 付款时间 3 发货时间 4 退款时间 5 退款完成时间 6 订单完成时间 7 订单取消时间",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "is_first_order",
                        "in": "query",
                        "description": "是否首单",
                        "required": false,
                        "type": "boolean"
                    },
                    {
                        "name": "type",
                        "in": "query",
                        "description": "订单类型：  \n object : 普通商品  \n integral: 积分商品  \n virtual: 第三方虚拟商品 (目前仅微众商城有此类)  \n delivery: 套餐商品  \n wzcard: 微众电子卡订单  \n thanks_card: 贺卡订单 ",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "supplier_type",
                        "in": "query",
                        "description": "订单供货商类型（仅自营平台具有供货商功能）：  \n 0 : 同步供货商  \n 1: 自建供货商  \n ",
                        "required": false,
                        "type": "integer"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "code": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "data": {
                                    "type": "object",
                                    "properties": {
                                        "orders": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/definitions/Order"
                                            }
                                        }
                                    }
                                },
                                "errMsg": {
                                    "type": "string"
                                },
                                "innerErrMsg": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "Order": {
            "type": "object",
            "properties": {
                "order_id": {
                    "type": "string",
                    "description": "订单编号."
                },
                "status": {
                    "type": "string",
                    "description": "订单状态"
                },
                "total_price": {
                    "type": "string",
                    "description": "订单金额（实付金额RMB）."
                },
                "ship_name": {
                    "type": "string",
                    "description": "收货人"
                },
                "ship_address": {
                    "type": "string",
                    "description": "收获地址"
                },
                "ship_tel": {
                    "type": "string",
                    "description": "收获电话"
                },
                "bill_type": {
                    "type": "string",
                    "description": "发票类型"
                },
                "bill": {
                    "type": "string",
                    "description": "发票信息"
                },
                "buyer_name": {
                    "type": "string",
                    "description": "购买人"
                },
                "product_count": {
                    "type": "string",
                    "description": "购买商品数量"
                },
                "payment_time": {
                    "type": "string",
                    "description": "支付时间"
                },
                "member_id": {
                    "type": "string",
                    "description": "会员id"
                },
                "member_is_subscribed": {
                    "type": "boolean",
                    "description": "会员是否关注"
                },
                "come": {
                    "type": "string",
                    "description": "订单来源：weizoom_mall：微众商城；mine_mall：本店"
                },
                "type": {
                    "type": "string",
                    "description": "订单类型"
                },
                "integral": {
                    "type": "string",
                    "description": "订单使用积分"
                },
                "pay_interface_type": {
                    "type": "string",
                    "description": "支付类型"
                },
                "pay_interface_name": {
                    "type": "string",
                    "description": "支付方式"
                },
                "order_status": {
                    "type": "string",
                    "description": "订单状态"
                },
                "express_company_name": {
                    "type": "string",
                    "description": "快递名称"
                },
                "express_number": {
                    "type": "string",
                    "description": "快递号"
                },
                "leader_name": {
                    "type": "string",
                    "description": "负责人"
                },
                "remark": {
                    "type": "string",
                    "description": "订单备注"
                },
                "postage": {
                    "type": "string",
                    "description": "快递费"
                },
                "created_at": {
                    "type": "string",
                    "description": "订单创建时间"
                },
                "delivery_time": {
                    "type": "string",
