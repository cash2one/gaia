{
    "swagger": "2.0",
    "info": {
        "version": "0.0.1",
        "title": "zeus API doc"
    },
    "paths": {
        "/mall/delivery/": {
            "put": {
                "description": "订单发货\n",
                "parameters": [
                    {
                        "name": "order_id",
                        "in": "query",
                        "description": "订单编号",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "express_company_name",
                        "in": "query",
                        "description": "快递名称",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "express_number",
                        "in": "query",
                        "description": "快递编号",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "leader_name",
                        "in": "query",
                        "description": "负责人",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "operator_name",
                        "in": "query",
                        "description": "操作人",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "code": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "data": {
                                    "type": "object",
                                    "properties": {
                                        "msg": {
                                            "type": "string",
                                            "description": "信息描述"
                                        },
                                        "result": {
                                            "type": "string",
                                            "description": "SUCCESS or FAILED"
                                        }
                                    }
                                },
                                "errMsg": {
                                    "type": "string"
                                },
                                "innerErrMsg": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "description": "订单发货信息修改\n",
                "parameters": [
                    {
                        "name": "order_id",
                        "in": "query",
                        "description": "订单编号",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "express_company_name",
                        "in": "query",
                        "description": "快递名称",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "express_number",
                        "in": "query",
                        "description": "快递编号",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "leader_name",
                        "in": "query",
                        "description": "负责人",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "operator_name",
                        "in": "query",
                        "description": "操作人",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "code": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "data": {
                                    "type": "object",
                                    "properties": {
                                        "msg": {
                                            "type": "string",
                                            "description": "信息描述"
                                        },
                                        "result": {
                                            "type": "string",
                                            "description": "SUCCESS or FAILED"
                                        }
                                    }
                                },
                                "errMsg": {
                                    "type": "string"
                                },
                                "innerErrMsg": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/mall/order/": {
            "post": {
                "description": "修改订单状态（目前仅支持完成）",
                "parameters": [
                    {
                        "name": "order_id",
                        "in": "query",
                        "description": "订单编号",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "action",
                        "in": "query",
                        "description": "动作（finish,cancel,refund）",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "operator_name",
                        "in": "query",
                        "description": "操作人",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "code": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "data": {
                                    "type": "object",
                                    "properties": {
                                        "msg": {
                                            "type": "string",
                                            "description": "信息描述"
                                        },
                                        "result": {
                                            "type": "string",
                                            "description": "SUCCESS or FAILED"
                                        }
                                    }
                                },
                                "errMsg": {
                                    "type": "string"
                                },
                                "innerErrMsg": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/mall/product_sales/": {
            "get": {
                "description": "获取商品销量",
                "parameters": [
                    {
                        "name": "product_ids",
                        "in": "query",
                        "description": "商品ID号",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "code": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "data":{
                                    "type": "object",
                                    "properties": {
                                      "product_sales": {

                                      }
                                    }
                                },
                                "errMsg": {
                                    "type": "string"
                                },
                                "innerErrMsg": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/account/zypt_info/": {
            "get": {
                "description": "获取自营平台的信息",
                "parameters": [

                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "code": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "data":{
                                    "type": "object",
                                    "properties": {
                                      "user_id": {
                                        "type": "integer",
                                        "description":"自营平台账户对应的user id"
                                      },
                                      "webapp_id": {
                                        "type": "string",
                                        "description": "自营平台账户对应的weapp id"
                                      },
                                      "store_name": {
                                        "type": "string",
                                        "description": "供应商名称"
                                      }
                                    }
                                },
                                "errMsg": {
                                    "type": "string"
                                },
                                "innerErrMsg": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/mall/supplier/": {
          "put": {
            "description": "添加供货商",
            "parameters": [
              {
                  "name": "owner_id",
                  "in": "query",
                  "description": "对应自营平台user id",
                  "required": true,
                  "type": "integer"
              },
              {
                  "name": "name",
                  "in": "query",
                  "description": "供货商的名称",
                  "required": true,
                  "type": "string"
              },
              {
                  "name": "responsible_person",
                  "in": "query",
                  "description": “负责人",
                  "required": true,
                  "type": "string"
              },
              {
                "name": "supplier_tel",
                "in": "query",
                "description": “供货商电话",
                "required": true,
                "type": "string"
              },
              {
                "name": "supplier_address",
                "in": "query",
                "description": “供货商地址",
                "required": true,
                "type": "string"
              },
              {
                "name": "remark",
                "in": "query",
                "description": “备注",
                "required": false,
                "type": "string"
              }
            ],
            "responses": {
              "200": {
                "description": "Successful response",
                "schema": {
                    "type": "object",
                    "properties": {
                        "code": {
                            "type": "integer",
                            "format": "int32"
                        },
                        "data": {
                            "type": "object",
                            "properties": {
                                "msg": {
                                    "type": "string",
                                    "description": "返回信息"
                                },
                                "supplier_id": {
                                    "type": "integer",
                                    "description": "创建的供货商的id"
                                }
                            }
                        },
                        "errMsg": {
                            "type": "string"
                        },
                        "innerErrMsg": {
                            "type": "string"
                        }
                    }
                }
              }
            }
          }
        },
        "/mall/product/": {
          "put": {
            "description": "创建商品",
            "parameters":[
              {
                "name": "owner_id",
                "in": "query",
                "description": "商品所属账号的user id",
                "required": true,
                "type": "integer"
              },
              {
                "name": "name",
                "in": "query",
                "description": "商品名称",
                "required": true,
                "type": "string"
              },
              {
                "name": "supplier",
                "in": "query",
                "description": "商品供货商id",
                "required": false,
                "type": "integer"
              },
              {
                "name": "purchase_price",
                "in": "query",
                "description": "商品采购价",
                "required": false,
                "type": "string"
              },
              {
                "name": "promotion_title",
                "in": "query",
                "description": "商品促销标题",
                "required": false,
                "type": "string"
              },
              {
                "name": "user_code",
                "in": "query",
                "description": "商品编码",
                "required": false,
                "type": "string"
              },
              {
                "name": "bar_code",
                "in": "query",
                "description": "商品条码",
                "required": false,
                "type": "string"
              },
              {
                "name": "product_category",
                "in": "query",
                "description": "将商品放入的分组当中",
                "required": false,
                "type": "string"
              },
              {
                "name": "min_limit",
                "in": "query",
                "description": "限购数量",
                "required": false,
                "type": "integer"
              },
              {
                "name": "is_member_product",
                "in": "query",
                "description": "是否参与会员折扣",
                "required": false,
                "type": "boolean"
              },
              {
                "name": "customModels",
                "in": "query",
                "description": "定制规格json字符窜",
                "required": false,
                "type": "string"
              },
              {
                "name": "is_use_custom_model",
                "in": "query",
                "description": "商品是否使用订制规格",
                "required": true,
                "type": "boolean"
              },
              {
                "name": "price",
                "in": "query",
                "description": "商品单价",
                "required": false,
                "type": "string"
              },
              {
                "name": "weight",
                "in": "query",
                "description": "商品重量",
                "required": false,
                "type": "string"
              },
              {
                "name": "stock_type",
                "in": "query",
                "description": "库存类型（有限或者无限）",
                "required": true,
                "type": "boolean"
              },
              {
                "name": "stocks",
                "in": "query",
                "description": "库存值",
                "required": false,
                "type": "integer"
              },
              {
                "name": "swipe_images",
                "in": "query",
                "description": "商品图片（json字符串）",
                "required": true,
                "type": "string"
              },
              {
                "name": "postage_type",
                "in": "query",
                "description": "运费类型",
                "required": true,
                "type": "string"
              },
              {
                "name": "unified_postage_money",
                "in": "query",
                "description": "统一运费金额",
                "required": true,
                "type": "string"
              },
              {
                "name": "is_enable_cod_pay_interface",
                "in": "query",
                "description": "是否支持货到付款",
                "required": false,
                "type": "boolean"
              },
              {
                "name": "is_use_online_pay_interface",
                "in": "query",
                "description": "是否支持在线支付",
                "required": false,
                "type": "boolean"
              },
              {
                "name": "is_enable_bill",
                "in": "query",
                "description": "是否支持开发票",
                "required": false,
                "type": "boolean"
              },
              {
                "name": "is_delivery",
                "in": "query",
                "description": "是否支持配送时间",
                "required": false,
                "type": "boolean"
              },
              {
                "name": "properties",
                "in": "query",
                "description": "商品属性（json）",
                "required": false,
                "type": "string"
              },
              {
                "name": "detail",
                "in": "query",
                "description": "商品详情",
                "required": false,
                "type": "string"
              }
            ],
            "responses": {
              "200": {
                "description": "Successful response"
              }
            }
          }
        },
        "/mall/user_list": {
          "get": {
                "description": "商户列表",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "需要第几页的数据，默认第一页",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "name": "page_count",
                        "in": "query",
                        "description": "每一页需要多少个商户数据，默认15条",
                        "required": true,
                        "type": "integer"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "code": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "data": {
                                    "type": "object",
                                    "properties": {
                                        "users": {
                                            "type": "array",
                                            "description": "商户集合",
                                            "items":{
                                                "type": "object",
                                                "properties": {
                                                   "user_id": {
                                                      "type": "integer",
                                                      "description": "商户的user_id"

                                                    },
                                                    "webapp_id": {
                                                      "type": "string",
                                                      "description": "商户的weapp_id"
                                                    },
                                                    "store_name": {
                                                      "type": "string",
                                                      "description": "商户的商店名"

                                                    }
                                                }
                                            }
                                        },
                                        "counts":{
                                          "type": "integer",
                                          "description": "总共有多少个有效的商户"
                                        }

                                    }
                                },
                                "errMsg": {
                                    "type": "string"
                                },
                                "innerErrMsg": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}