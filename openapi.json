                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "express_company_name",
                        "in": "query",
                        "description": "快递名称",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "express_number",
                        "in": "query",
                        "description": "快递编号",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "leader_name",
                        "in": "query",
                        "description": "负责人",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "operator_name",
                        "in": "query",
                        "description": "操作人",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "code": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "data": {
                                    "type": "object",
                                    "properties": {
                                        "msg": {
                                            "type": "string",
                                            "description": "信息描述"
                                        },
                                        "result": {
                                            "type": "string",
                                            "description": "SUCCESS or FAILED"
                                        }
                                    }
                                },
                                "errMsg": {
                                    "type": "string"
                                },
                                "innerErrMsg": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "description": "订单发货信息修改\n",
                "parameters": [
                    {
                        "name": "order_id",
                        "in": "query",
                        "description": "订单编号",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "express_company_name",
                        "in": "query",
                        "description": "快递名称",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "express_number",
                        "in": "query",
                        "description": "快递编号",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "leader_name",
                        "in": "query",
                        "description": "负责人",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "operator_name",
                        "in": "query",
                        "description": "操作人",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "code": {
                                    "type": "integer",
                                    "format": "int32"
                                },
                                "data": {
                                    "type": "object",
                                    "properties": {
                                        "msg": {
                                            "type": "string",
                                            "description": "信息描述"
                                        },
                                        "result": {
                                            "type": "string",
                                            "description": "SUCCESS or FAILED"
                                        }
                                    }
                                },
                                "errMsg": {
                                    "type": "string"
                                },
                                "innerErrMsg": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
