                    "type": "string",
                    "description": ""
                },

                "product_id": {
                    "type": "string",
                    "description": "商品id"
                },
                "promote_status": {
                    "type": "string",
                    "description": "推广状态 1: 推广中 2: 推广结束"
                },
                "promote_money": {
                    "type": "string",
                    "description": "推广费用/件"
                },
                "promote_stock": {
                    "type": "string",
                    "description": "推广库存"
                },
                "promote_time_to": {
                    "type": "string",
                    "description": "推广结束时间%Y-%m-%d %H:%M:%S"
                },
                "promote_time_from": {
                    "type": "string",
                    "description": "推广开始时间%Y-%m-%d %H:%M:%S"
                },
                "promote_total_money": {
                    "type": "string",
                    "description": "推广费用总费用"
                },
                "promote_sale_count": {
                    "type": "string",
                    "description": "推广销量"
                },

                "created_at": {
                    "type": "string",
                    "description": "创建时间%Y-%m-%d %H:%M:%S"
                }

            }
        },
        "ProductPromoteInList":{
            "type": "object",
            "properties": {
                "promote_info": {
                    "$ref": "#/definitions/ProductPromote"
                },

                "product_info": {
                    "$ref": "#/definitions/ProductInfo"
                }


            }
        },
        "ProductInfo": {
            "type": "object",
            "description": "商品的信息",
            "properties": {
                "name": {
                    "type": "string",
                    "description": "商品名"
                },
                "type": {
                    "type": "string",
                    "description": "商品类型",
                    "format": "默认为object"
                },
                "create_type": {
                    "type": "string",
                    "description": "商品的创建类型",
                    "format": "'create(自建商品)'或'sync(从商品池同步商品)'"
                },
                "supplier_id": {
                    "type": "number",
                    "description": "供应商的id，可以不提供，默认为0，表示是自建商品"
                },
                "classification_id": {
                    "type": "number",
                    "description": "商品分组的id，可以不提供，默认为0，表示没有商品分组"
                },
                "bar_code": {
                    "type": "string",
                    "description": "商品条码"
                },
                "min_limit": {
                    "type": "number",
                    "description": "起购数量"
                },
                "promotion_title": {
                    "type": "string",
                    "description": "促销标题"
                },
                "is_enable_bill": {
                    "type": "boolean",
                    "description": "是否出具发票"
                },
                "is_member_product": {
                    "type": "boolean",
                    "description": "是否参与会员折扣"
                },
                "detail": {
                    "type": "string",
                    "description": "商品详情"
                },
                "product_models_info": {
                    "$ref": "#/definitions/ModelsInfo"
                },
                 "labels": {
                    "$ref": "#/definitions/Lables"
                },
                "image": {
                    "type": "string",
                    "description": "商品缩略图的url"
                },

                "categories": {
                    "$ref": "#/definitions/ReturnCategories"
                },
                "classifications": {
                    "$ref": "#/definitions/ReturnClassifications"
                },
                "supplier": {
                    "$ref": "#/definitions/ReturnSupplier"
                }
            }
        }

